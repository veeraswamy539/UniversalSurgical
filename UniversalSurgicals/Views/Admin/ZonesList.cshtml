<script type="text/javascript">
    $("#updateZoneclse").click(function () {
        $("#modal-161").fadeOut();
        $(".md-overlay").fadeOut();

    });

    $('#searchBtn').click(function (e) {
        e.preventDefault();
        // alert();
        if ($('#SearchString').val() != "") {
            $("#AdminLoadingBg").fadeIn();
            $("#AdminLoadimg").show();
            $.ajax({
                url: "/Admin/ZonesList",
                type: 'GET',
                cache: false,
                data: { searchString: $('#SearchString').val() },

                success: function (result) {
                    $("#AdminLoadingBg").fadeOut();
                    $("#AdminLoadimg").hide();
                    $('#divPartial').html(result);
                }
            });
        }
        else {
            return;
        }
        return false;
    });

    $(function () {
        $('#sortdiv').on('click', 'a', function (e) {
            e.preventDefault();
            $("#AdminLoadingBg").fadeIn();
            $("#AdminLoadimg").show();
            $.ajax({
                url: this.href,
                type: 'GET',

                cache: false,
                success: function (result) {
                    $("#AdminLoadingBg").fadeOut();
                    $("#AdminLoadimg").hide();
                    $('#divPartial').html(result);
                  
                }
            });
            return false;
        });
    })

    $(function () {
        $('#myPager').on('click', 'a', function (e) {
            e.preventDefault();

            if (this.href != "") {
                $("#AdminLoadingBg").fadeIn();
                $("#AdminLoadimg").show();
                $.ajax({
                    url: this.href,
                    type: 'GET',
                    cache: false,
                    success: function (result) {
                        $("#AdminLoadingBg").fadeOut();
                        $("#AdminLoadimg").hide();
                        $('#divPartial').html('');
                        $('#divPartial').empty();
                        $('#divPartial').html(result);
                        
                    }
                });
            }
        });
    });
    $(".md-overlay").click(function (e) {
        $('#modal-161').removeClass('md-show');
    });

    $("#zonepopclose").click(function () {
        $('#modal-161').removeClass('md-show');
        $("#modal-16").fadeOut();
        $(".md-overlay").fadeOut();
        $("#txtZoneName-error").empty();


        $("#txtDesc-error").empty();

        $('#txtZoneName,#txtDesc').val("");



    });
    $(".update").click(function (e) {
        e.preventDefault();
        $("#modal-161").fadeIn();
        $(".md-overlay").fadeIn();
        var redirecturl = $(this).attr('href');
        $.ajax({
            url: redirecturl,
            contentType: 'application/html; charset=utf-8',
            type: 'GET',
            cache: false,
            dataType: 'html'

        })
       .success(function (result) {
           if (result.indexOf("Session") != -1) {
               $("#modal-161").fadeOut();
               $(".md-overlay").fadeOut();

               $('#divPartial').html('');
               $('#divPartial').html(result);
           }
           else {
               $('#modal-161').show();
               $('#modal-161').addClass('md-modal md-effect-16 md-show');
               $('#uploaddiv').html('');
               $('#uploaddiv').html(result);
           }
       })
       .error(function (xhr, status) {
       });
    });

</script>
<script type="text/javascript">
    $(document).ready(function () {
        $(this).scrollTop(0);
        $("#btnCreateZone").click(function () {

            $("#modal-16").fadeIn();
            $(".md-overlay").fadeIn();
        });
        $("#TotalZonesList").show();
        $("#Zonelistsearch").show();
        var targetrouter = '@Session["url"]'
        $(".zoneclick").click(function (e) {
            e.preventDefault();
            $("#AdminLoadingBg").fadeIn();
            $("#AdminLoadimg").show();
            $('#EmployeePartialDev').empty();
            $('#EmployeePartialDev').val("");
            var redirecturl = $(this).attr('href');
            $.ajax({
                cache: false,
                type: 'POST',
                url: '/Home/SessionOut',
                dataType: 'json',
                success: function (data) {
                  
                    if (data.Text === "session") {
                        $.ajax({
                            url: redirecturl,
                            contentType: 'application/html; charset=utf-8',
                            type: 'GET',
                            dataType: 'html'

                        })
       .success(function (result) {
           $("#AdminLoadingBg").fadeOut();
           $("#AdminLoadimg").hide();
           $('#EmployeePartialDev').empty();
           $('#EmployeePartialDev').val("");
           $('#EmployeePartialDev').show();
           //  alert(result);
           $('#EmployeePartialDev').html(result);
       })
       .error(function (xhr, status) {
           // alert("2");
           $("#AdminLoadingBg").fadeOut();
           $("#AdminLoadimg").hide();
       });
                    }
                    else {
                        window.location.href = "/Home/Logout";
                    }
                },
                failure: function () {
                    alert("fail");
                    $("#AdminLoadingBg").fadeOut();
                    $("#AdminLoadimg").hide();
                }
            });
        });
    });
</script>
