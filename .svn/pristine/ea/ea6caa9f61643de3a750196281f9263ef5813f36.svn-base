





@using Universal.BusinessEntities;
@model UserExpenses_Header
   @Scripts.Render("~/bundles/jqueryval")

<div class=" container-fluid emp-exd-frm margin-20 p-b-20">




    <div class="grid-hd col-lg-12 col-md-12 col-sm-12" style="background-color:#333645; color:#fff;">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 " >Expenditure List</div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 text-right">
                <div class="col-md-4 emp-exp-dt pull-right text-right">
                    <div class="clearfix"></div>
                </div>
            </div>
    </div>

    <div class="clearfix"></div>
    <br />
        @if (Model.UserExpenseDetailsList.Count != 0)
        {
            <table class="table no-more-tables col-lg-12">
                <thead>
                    <tr>
                        <th class="col-lg-3 col-md-3">Expense Type</th>
                        <th class="col-lg-3 col-md-3">From</th>
                        <th class="col-lg-3 col-md-3">To</th>
                        <th class="col-lg-3 col-md-3">Travelled Kms</th>
                        <th class="col-lg-3 col-md-3">Amount</th>
                    </tr>
                </thead>
                <tbody>
                @foreach (var item in Model.UserExpenseDetailsList)
                {
                    <tr>
                        <td class="v-align-middle">@item.ExpenseTypeDetails</td>
                        <td class="v-align-middle">@item.FromLocation</td>
                        <td class="v-align-middle">@item.ToLocation</td>
                        <td class="v-align-middle">@item.TraveledKms</td>
                        <td class="v-align-middle">@item.Amount</td>
                    </tr>
                }
                </tbody>
            </table>
            <div  class="col-lg-12 col-md-12"></div>
        }
        else
        {
             <div class="col-lg-12 col-md-12 m-t-20 m-l-20 norecordsfound">
             <img src="~/Content/Images/norecordfound.jpg" alt="No Records Found" title="No Records Found">
            </div>
        }
</div>
